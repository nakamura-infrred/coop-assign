{
  "name": "coop-assign",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @coop-assign/web dev",
    "build": "pnpm --filter @coop-assign/web build",
    "lint": "pnpm --recursive --if-present run lint",
    "test": "pnpm --recursive --if-present run test",
    "deploy": "pnpm --filter @coop-assign/web build && firebase deploy --config firebase.json --only hosting",
    "seed:prepare": "node scripts/prepare-seeds-from-team-place.mjs",
    "seed:push": "node scripts/push-firestore-seeds.mjs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@holiday-jp/holiday_jp": "^2.5.1",
    "firebase-admin": "^12.5.0",
    "slugify": "^1.6.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "playwright": "1.49.1"
  }
}
